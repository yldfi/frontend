# WalletConnect Project ID
# Get yours at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Enso Finance API Key (optional, for higher rate limits)
# Get yours at https://developers.enso.build
NEXT_PUBLIC_ENSO_API_KEY=

# Cloudflare API Token (for wrangler deploy)
# Has permissions: Workers Scripts, Workers KV, Workers Routes
CLOUDFLARE_API_TOKEN=

# Tenderly Simulation API (server-side only)
TENDERLY_ACCESS_KEY=
TENDERLY_ACCOUNT_SLUG=
TENDERLY_PROJECT_SLUG=

# Nonce signing secret for /api/simulate
SIMULATION_NONCE_SECRET=

# Debug RPC for local development (with debug_traceCall support)
# Used to trace Enso bundle transactions for debugging
DEBUG_RPC_URL=
DEBUG_RPC_AUTH=

# Etherscan API Key (for integration tests - token holder lookups)
# Get yours at https://etherscan.io/myapikey
ETHERSCAN_API_KEY=

# Moralis API Key (server-side only - used by /api/token-holders endpoint)
# Get yours at https://moralis.io/
MORALIS_API_KEY=

# Token Holders API (for integration tests)
# Shared secret for authenticating with /api/token-holders endpoint
# Generate with: openssl rand -base64 32
TOKEN_HOLDER_API_KEY=

# Token Holders API URL (defaults to localhost:3000 for local dev)
# Set to production URL in CI: https://your-domain.com/api/token-holders
TOKEN_HOLDER_API_URL=
